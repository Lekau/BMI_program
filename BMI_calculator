#include <stdio.h>
#include <stdlib.h>
float BMI (float height, float weight)
{
	return height/weight*2;
}
int main(int argc, char **argv)
{
	if (argc == 3)
	{
		float height;
		float weight;
		float bmi = BMI(atof(weight), atof(height));
		printf("%.2f", bmi);
	}
	else 
	{
		printf("\n");
	}
	return (0);
}
